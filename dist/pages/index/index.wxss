@import "./../../components/minetaskbar/MineTaskBar.wxss";
@import "./../../components/taskbar/TaskBar.wxss";
.container {
  min-height: 100%;
  background: #eee;
}
.task_list {
  margin-bottom: 50px;
  margin-left: 8px;
  margin-right: 8px;
}
.task_school {
  height: 50px;
  border-top: 1px solid #eee;
  background: #fff;
  font-size: 13px;
  color: #999;
  text-align: center;
  line-height: 50px;
}
.task_school Text {
  margin-left: 10px;
}
.task_title {
  padding: 0 5px;
  height: 30px ;
  margin-top: 30px;
  color: #999;
  font-size: 13px;
  margin-left: 15px;
  display: flex;
}
.task_title Text {
  text-align: center;
  width: 25%;
}
.data_more {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.data_more van-loading {
  position: absolute;
  top: -40px;
}
.task_wrapper {
  margin-top: 20px;
}
